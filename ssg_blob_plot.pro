pro ssg_blob_plot

  init = {tok_sysvar}
  ;; 9 hr 55 min 29.71
  sysIII = 9*3600. + 55*60. + 29.71
  t = findgen(100)/10.* 3600.*24
  sysIII_ang = t/sysIII*360
  cirrange, sysIII_ang
  ;; print, sysIII_ang
  plot, [0,0], psym=dot, $
        xrange=[1,10], yrange=[0,720], $
        ytickinterval=90, yminor=9, $
        ystyle=!tok.exact, xtitle='Day of 1998-10'
  oplot, [1.21680], [140+360], psym=!tok.square
  oplot, [1.31714], [210+360], psym=!tok.square
  oplot, [1.45996], [305+360], psym=!tok.square
  oplot, [5.30786], [345], psym=!tok.square
  oplot, [6.29468], [285], psym=!tok.square
  oplot, [8.23804], [140], psym=!tok.square
  oplot, [8.26001], [160], psym=!tok.square
  ;;oplot, [8.38135], [235], psym=!tok.square
  oplot, [8.44849], [280], psym=!tok.square
  oplot, [9.29712], [130], psym=!tok.square
  oplot, [9.40942], [205], psym=!tok.square
  oplot, [10.1843], [360], psym=!tok.square

  ;;
  ;;  1998-10-1	& 		& 140?, 210? 305?\\
  ;;1998-10-5	& 		& 345\\
  ;;1998-10-6	& 		& 285 \\
  ;;1998-10-7	& 		& - \\
  ;;1998-10-8	& 		& 140, 235?, 285\\
  ;;1998-10-9	& 		& 130, 205\\
  ;;1998-10-10	& 		& 0 \\

end


